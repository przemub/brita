#: ../brita.py:130
#, python-format
msgid "Brita %s - backlight adjuster for Linux"
msgstr "Brita %s - program zarządzający podświetleniem ekranu dla Linuksa"

#: ../brita.py:132
msgid ""
"Usage: brita [options] value\n"
"\tOptions:\n"
"\t  -q, --quiet\tDisable printing output\n"
"\t  -v, --version\tPrint the current version and exit\n"
"\t  -h, --help\tPrint this message and exit\n"
"\t  -m, --min\tDon't set the brightness to a value lower than 100\n"
"\n"
"\tValue can be given in percents (ie. 50%)\n"
"\t\tor relative to the current value (ie. -500, +30%, -10%)\n"
"\n"
"\tProgram executed without arguments prints the current brightness."
msgstr ""
"Użycie: brita [opcje] wartość\n"
"\tOpcje:\n"
"\t  -q, --quiet\tWyłącz wypisywanie wyjścia\n"
"\t  -v, --version\tWyświetl aktualną wersję\n"
"\t  -h, --help\tWyświetl pomoc\n"
"\t  -m, --min\tNie ustawiaj jasności na wartości niższe od 100\n"
"\n"
"\tWartość może być podana w procentach (ie. 50%)\n"
"\t\tlub relatywnie do obecnej (np. -500, +30%, -10%)\n"
"\n"
"\tProgram wywołany bez wartości pokazuje obecnie ustawioną jasność."

